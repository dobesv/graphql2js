  "version": "1.0.0",
  "description": "Generate js files from .graphql files, so you can import them",
  "main": "bin/index.js",
  "author": "Dobes Vandermeer",
  "license": "MIT",
  "scripts": {
    "build": "tsc && chmod +x bin/index.js"
